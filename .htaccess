RewriteEngine On

# Rewrite rules for each page
RewriteRule ^login$ /auth/login.php [L]
RewriteRule ^register$ /auth/register.php [L]
RewriteRule ^admin$ /admin/index.php [L]
RewriteRule ^logout$ /auth/logout.php [L]
RewriteRule ^home$ /index.php [L]
RewriteRule ^about-us$ /users/page/about.php [L]
RewriteRule ^courses$ /users/page/courses.php [L]
RewriteRule ^contact-us$ /users/page/contact.php [L]
RewriteRule ^education$ /users/page/education.php [L]
RewriteRule ^experience$ /users/page/experience.php [L]
RewriteRule ^skills$ /users/page/skills.php [L]
RewriteRule ^testimonial$ /users/page/testimonial.php [L]
RewriteRule ^404$ /404.php [L]